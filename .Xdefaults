xterm*faceName: mononoki
xterm*faceSize: 16
xterm*foreground: gray
xterm*background: black
xterm*utf8: 1
xterm*eightBitInput: false
xterm*metaSendsEscape: false
xterm*selectToClipboard: true

URxvt.background: #000000
URxvt.foreground: gray90
URxvt.visualBell: true
URxvt.mapAlert: true
URxvt*font: xft:mononoki:style=Regular:size=16,                                   \
			xft:Inconsolata:style=Regular:size=16,                                \
			xft:DejaVu Sans Mono:style=Book:size=16,                              \
			xft:DejaVu Serif:style=Book:size=16,                                  \
			xft:Liberation Mono:style=Regular:size=16,                            \
			xft:Symbola:style=Regular:size=16,                                    \
			xft:PowerlineSymbols:style=Medium:size=16,                            \
			xft:Unifont Sample:style=Medium,                                      \
			xft:Unifont:style=Medium,                                     
URxvt*scrollBar: false
URxvt.perl-ext-common: default,matcher,font-size,url-select,keyboard-select,selection-to-clipboard
URxvt.keysym.C-h: perl:font-size:increase
URxvt.keysym.C-l: perl:font-size:decrease
URxvt.keysym.C-0: perl:font-size:reset
URxvt.keysym.M-u: perl:url-select:select_next
!! keyboard-select:
!!URxvt.keysym.M-Escape: perl:keyboard-select:activate
!! Matcher
URxvt.url-select.launcher: firefox-bin
URxvt.url-select.underline: true
URxvt.url-launcher:	firefox-bin
URxvt.matcher.button:	1
